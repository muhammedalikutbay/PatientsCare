{
    "AppSettings": {
      "AppTitle": "PatientCare",
      "Version": "1.0.0",
      "LogLevel": "Information",
      "MaxItemsPerPage": 20
    },
    "ConnectionStrings": {
      "DefaultConnection": "Data Source=HastaTakipDB.db;Version=3;"
    },
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "Authentication": {
      "Jwt": {
        "SecretKey": "your-256-bit-secret",
        "Issuer": "PatientCareApp",
        "Audience": "PatientCareUsers",
        "ExpirationInMinutes": 60
      }
    },
    "ExternalApis": {
      "ExternalServiceApi": {
        "BaseUrl": "https://api.externalservice.com",
        "ApiKey": "your-api-key-here"
      }
    },
    "EmailSettings": {
      "SmtpServer": "smtp.yourmailserver.com",
      "Port": 587,
      "Username": "your-email@example.com",
      "Password": "your-email-password",
      "FromEmail": "your-email@example.com"
    },
    "AllowedHosts": "*"
  }
  